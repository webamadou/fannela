<p id="notice"><%= notice %></p>

<h1>Admin Countries</h1>
<table id="dttb" class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Code iso</th>
      <th>State</th>
      <th>&nbsp;</th>
    </tr>
  </thead>

  <tbody>
    <% @admin_countries.each do |admin_country| %>
      <tr>
        <td><%= admin_country.name %></td>
        <td><%= admin_country.code_iso %></td>
        <td><%= admin_country.state %></td>
        <td>
          <%= link_to 'Show', admin_country %> -
          <%= link_to 'Edit', edit_admin_country_path(admin_country) %> -
          <%= link_to 'Destroy', admin_country, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Admin Country', new_admin_country_path %>
